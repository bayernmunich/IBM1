<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
  PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
  "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
 	<servlet>
		<servlet-name>HealthClassServlet</servlet-name>
		<display-name>HealthClassServlet</display-name>
		<servlet-class>com.ibm.ws.console.healthconfig.servlet.HealthClassServlet</servlet-class>		
	</servlet>
	
	<servlet>
		<servlet-name>HealthCustomActionServlet</servlet-name>
		<display-name>HealthCustomActionServlet</display-name>
		<servlet-class>com.ibm.ws.console.healthconfig.servlet.HealthCustomActionServlet</servlet-class>		
	</servlet>
	
	<servlet-mapping>
        <servlet-name>HealthClassServlet</servlet-name>
        <url-pattern>/healthclass</url-pattern>
    </servlet-mapping> 
    
    
	<servlet-mapping>
        <servlet-name>HealthCustomActionServlet</servlet-name>
        <url-pattern>/customprocessdefs</url-pattern>
    </servlet-mapping> 
    
    <security-constraint>
      <web-resource-collection>
         <web-resource-name>Protected Area</web-resource-name>
         <url-pattern>/healthclass/*</url-pattern>
         <http-method>DELETE</http-method>
         <http-method>GET</http-method>
         <http-method>POST</http-method>
         <http-method>PUT</http-method>
      </web-resource-collection>
      <auth-constraint>
         <role-name>administrator</role-name>
         <role-name>operator</role-name>
         <role-name>configurator</role-name>
         <role-name>monitor</role-name>
      </auth-constraint>
      <user-data-constraint>
         <transport-guarantee>CONFIDENTIAL</transport-guarantee>
      </user-data-constraint>
   </security-constraint>
   
    <security-constraint>
      <web-resource-collection>
         <web-resource-name>Protected Area</web-resource-name>
         <url-pattern>/customprocessdefs/*</url-pattern>
         <http-method>DELETE</http-method>
         <http-method>GET</http-method>
         <http-method>POST</http-method>
         <http-method>PUT</http-method>
      </web-resource-collection>
      <auth-constraint>
         <role-name>administrator</role-name>
         <role-name>operator</role-name>
         <role-name>configurator</role-name>
         <role-name>monitor</role-name>
      </auth-constraint>
      <user-data-constraint>
         <transport-guarantee>CONFIDENTIAL</transport-guarantee>
      </user-data-constraint>
   </security-constraint>
       
</web-app>
