<!DOCTYPE web-app
  PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
  "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
    <servlet>
        <servlet-name>MiddlewareServerStatusServlet</servlet-name>
        <servlet-class>com.ibm.ws.console.middlewareserver.MiddlewareServerStatusServlet</servlet-class>
    </servlet>

	<servlet>
        <servlet-name>MiddlewareServerMaintModeStatusServlet</servlet-name>
        <servlet-class>com.ibm.ws.console.middlewareserver.MiddlewareServerMaintModeServlet</servlet-class>
	</servlet>
	
    <servlet-mapping>
        <servlet-name>MiddlewareServerStatusServlet</servlet-name>
        <url-pattern>/middlewareserverstatus</url-pattern>
    </servlet-mapping>


	<servlet-mapping>
        <servlet-name>MiddlewareServerMaintModeStatusServlet</servlet-name>
        <url-pattern>/serverMaintStatus</url-pattern>
    </servlet-mapping>


</web-app>
