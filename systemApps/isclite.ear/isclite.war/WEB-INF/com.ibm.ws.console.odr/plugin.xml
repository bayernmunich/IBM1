	<extension point="com.ibm.websphere.wsc.form">
    	<form id="ODR.config.general.properties" contextType="ODR">
	     <field attributeName="developmentMode" perspective="config">
	     </field>	
	     <field attributeName="parallelStartEnabled" perspective="config">
	     </field>
	     <!-- Begin  z/OS adaptive fields -->
         <field
              attributeName="shortName"
              perspective="config">
              <compatibility>
                 <match key="com.ibm.websphere.nodeOperatingSystem"  value="(os390)"/>
              </compatibility>
         </field >
         <field
              attributeName="uniqueId"
              perspective="config">
              <compatibility>
                 <match key="com.ibm.websphere.nodeOperatingSystem"  value="(os390)"/>
              </compatibility>
      </field >
         <!-- End  z/OS adaptive fields -->
         <!-- LIDB2727 Begin -->
         <field
              attributeName="runIn64bitJVMMode"
              perspective="config">
             <compatibility>
                <match key="com.ibm.websphere.nodeOperatingSystem"  value="(os390)"/>
                <match key="com.ibm.websphere.baseProductVersion" value="((\d\d)|([7-9])|(6\.((\d\d)|[2-9]))|(6\.1\.[1-9])|(6\.1\.0\.((\d\d)|[4-9]))).*"/>
             </compatibility>
         </field >
         <!-- LIDB2727 End-->
         <field
              attributeName="provisionComponents"
              perspective="config">
              <compatibility>
              	 <match key="com.ibm.websphere.baseProductMajorVersion"  value="[7-9]"/>
              </compatibility>
         </field>
     	</form>
	</extension>
