	<extension point="com.ibm.websphere.wsc.collectionItem">
		<collectionItem id="odr.Proxy.collectionItem1" contextType="ODR">
			<item
				attributeName="name"
				sortable="sort"
				itemURI="proxyCollection.do?EditAction=true"
				label="ManagedObject.name.displayName">
			</item>
			<item attributeName="node" sortable="sort" itemURI=""
				label="Server.node.displayName">
			</item>
            <item attributeName="version" sortable="sort" itemURI=""
                  label="node.version.displayName">
                  <compatibility>
					<match key="com.ibm.websphere.baseProductVersion"
                        value="(6\.[1-9].*.*)|(([7-9]|[1-9][0-9]).*.*.*)"/>
				</compatibility>    	
            </item>
			<item attributeName="protocol" sortable="sort" itemURI=""
				label="ProxyServer.transport.protocol.displayName">
				<compatibility>
					<match key="com.ibm.websphere.baseProductVersion"
                        value="(6\.[1-9].*.*)|(([7-9]|[1-9][0-9]).*.*.*)"/>
				</compatibility>    	
			</item>			
			<item attributeName="status" sortable="sort" itemURI=""
				label="Server.status.displayName">
			</item>
		</collectionItem>
	</extension>
	
