<project name="createProxyStaticRoutingDir" default="createProxyStaticRoutingDir" basedir=".">
    
    <target name="createDir"
            description="Creates the staticContent directory under the profile root">            
    	<mkdir dir="${profilePath}/staticContent" />        
    </target>

    <target name="createProxyStaticRoutingDir"
        description="Creates default static file serving directory for the proxy server"
        depends="createDir"/>
</project>
