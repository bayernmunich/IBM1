<?xml version='1.0' encoding='utf-8'?>

<!--  WorkSpace Meta Data for Single Server  -->

<metadata>
	<metadata-name>SingleServer</metadata-name>
    	<root-context-type></root-context-type>
    
	<contexts>
		<context>
			<context-name>cells</context-name>
			<child-document-names>
				<child-document-name>SIP Application Routers</child-document-name>
			</child-document-names>
		</context>
		<context>
			<context-name>clusters</context-name>
			<child-document-names>
				<child-document-name>CEA Extension</child-document-name>
			</child-document-names>
		</context>
		<context>
			<context-name>servers</context-name>
			<child-document-names>
				<child-document-name>CEA Extension</child-document-name>
			</child-document-names>
		</context>
	</contexts>   

    	<documents>
        	<document>
            	<document-name>SIP Application Routers</document-name>
            	<file-pattern>sipapplicationrouters.xml</file-pattern>
            	<root-refobject-types>
                		<root-refobject-type>SIPApplicationRouters</root-refobject-type>
            	</root-refobject-types>
        	</document>
		<document>
            	<document-name>CEA Extension</document-name>
            	<file-pattern>cea-extension.xml</file-pattern>
            	<root-refobject-types>
                		<root-refobject-type>CEASettings</root-refobject-type>
            	</root-refobject-types>
           	</document>
   	</documents>

</metadata>

