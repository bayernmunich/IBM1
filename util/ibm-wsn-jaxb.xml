<?xml version="1.0" encoding="UTF-8"?>
<!-- File to bind WS-Notification XML elements to customized IBM helper classes -->
<jxb:bindings version="1.0"
               xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <jxb:bindings schemaLocation="http://docs.oasis-open.org/wsn/b-2.xsd">
    <jxb:bindings node="//xs:complexType[@name='TopicExpressionType']">
      <jxb:class ref="com.ibm.websphere.sib.wsn.jaxb.base.TopicExpressionType"/>
    </jxb:bindings>
    <jxb:bindings node="//xs:complexType[@name='FilterType']">
      <jxb:class ref="com.ibm.websphere.sib.wsn.jaxb.base.FilterType"/>
    </jxb:bindings>
    <jxb:bindings node="//xs:complexType[@name='QueryExpressionType']">
      <jxb:class ref="com.ibm.websphere.sib.wsn.jaxb.base.QueryExpressionType"/>
    </jxb:bindings>        
  </jxb:bindings>      
</jxb:bindings>