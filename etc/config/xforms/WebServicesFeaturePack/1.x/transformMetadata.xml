<?xml version="1.0" encoding="UTF-8"?>
<!-- Have to copy all the WS config files that are filtered in the 6.1.x transformMetadata.xml
     because otherwise the SM framework will use 6.1's transformMetadata.xml to delete them -->
<transformMappings>
  <customTransform>
    <document>policySet.xml</document>
    <scope>PolicySets</scope>
    <className>com.ibm.ws.policyset.transform.PolicySetConfigTransformer61</className>
    <classPath>$(was.install.root)/plugins</classPath>
  </customTransform>
  <customTransform>
    <document>policy.xml</document>
    <scope>PolicyTypes</scope>
    <className>com.ibm.ws.policyset.transform.PolicyConfigTransformer61</className>
    <classPath>$(was.install.root)/plugins</classPath>
  </customTransform>
  <customTransform>
    <document>bindings.xml</document>
    <scope>PolicyTypes</scope>
    <className>com.ibm.ws.policyset.transform.BindingsConfigTransformer61</className>
    <classPath>$(was.install.root)/plugins</classPath>
  </customTransform>
  <transformMap>
    <document>security.xml</document>
    <transform>security.xsl</transform>
  </transformMap>
  <transformMap>
    <document>policyAttachments.xml</document>
    <transform>copy.xsl</transform>
  </transformMap>
  <transformMap>
    <document>clientPolicyAttachments.xml</document>
    <transform>copy.xsl</transform>
  </transformMap>
  <transformMap>
    <document>stsplugins.xml</document>
    <transform>stsplugins.xsl</transform>
  </transformMap>
  <transformMap>
    <document>ststargets.xml</document>
    <transform>ststargets.xsl</transform>
  </transformMap>
  <transformMap>
    <document>stsconfig.xml</document>
    <transform>stsconfig.xsl</transform>
  </transformMap>
  <transformMap>
    <document>WSSCCache.xml</document>
    <transform>copy.xsl</transform>
  </transformMap>
  
</transformMappings>
