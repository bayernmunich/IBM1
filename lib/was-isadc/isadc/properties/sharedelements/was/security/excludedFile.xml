<filter xmlns="http://www.ibm.com/autopd/filter" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xsi:schemaLocation="http://www.ibm.com/autopd/filter ../filterFile.xsd ">
	<fileEntry fileName="security.xml" fileType="XML" nameMatchPattern="name" >
		<match attributeType="name" matchType="xPath" value="//repertoire/setting/@keyFileName" caseSensitive="false"/>
		<match attributeType="name" matchType="xPath" value="//keyStores/@location" caseSensitive="false"/>
	</fileEntry>	 	
